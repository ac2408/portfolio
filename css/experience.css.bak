    background: linear-gradient(rgba(255,255,255,0.5), rgba(255,255,255,0.15), rgba(0,0,0,0.4), rgba(0,0,0,0.6)), url(../image/background/JojoLaPasshonejpg.jpg);
    background-size: cover;
    margin-top: -50px;
    color: antiquewhite;
}
.interest h1{
    margin-left: 20px;
    padding-top: 75px;
}

.interest h2{
    margin-left: 100px;
    font-size: 45px;
}

.interest p{
    margin-left: 200px;
    font-size: 25px
}

.interest-wrap {
    margin: 0 auto;
    padding: 2rem 5rem 4rem;
    overflow: auto;
}