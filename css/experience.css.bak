.experience{
    width: 100%;
    height: 100vh;
}

.background3{
    display: inline-block;
    width: 33.2%;
    height: 100vh;
    background: linear-gradient(rgba(255, 255, 255, 0.5), rgba(255, 255, 255, 0.05), rgba(0, 0, 0, 0.09), rgba(0, 0, 0, 0.4)), slategrey;
    background-size: cover;
}



.background3 h2{
    color: black;
    text-align: center;
    padding: 200px 0;
    margin: auto;
    margin-top: auto;
}

.background3 p{
    font-size: 25px;
    color: floralwhite;
    text-align: center;
    margin: auto;
    margin-top: auto;
}

.projet{
    width: 100%;
    height: 100vh;
    margin-top: 5px;
}

.background4{
    display: inline-block;
    width: 100%;
    height: 100vh;
    background: linear-gradient(rgba(255,255,255,0.5), rgba(255,255,255,0.05), rgba(0,0,0,0.09), rgba(0,0,0,0.4)),
    url(../image/background/gestion-de-projet.png) no-repeat fixed;
    background-size: cover;
}


.proj{
    display: inline-block;
    width: 49%;
    height: 10vh;
    margin-top: 100px;
}
.background4 h1{
    font-family: 'Mukta', sans-serif;
    color: darkslategrey;
    text-align: center;
    justify-content: center
}



.background4 h2{
    color: #83c0ef;
    text-align: center;
    margin-bottom: 20px;
    line-height: 50px;
    padding-top: 200px;
    font-size: 40px;
    margin: auto;
}

.background4 p{
    color: beige;
    text-align: center;
    margin-bottom: 0;
    margin-top: 0;
    font-size: 20px;
    margin: auto    ;
}




.background5{
    display: inline-block;
    width: 100%;
    height: 100vh;
    background: linear-gradient(rgba(255,255,255,0.5), rgba(255,255,255,0.05), rgba(0,0,0,0.09), rgba(0,0,0,0.4)),
    lightskyblue no-repeat fixed;
    background-size: cover;
    margin-bottom: 0;
}
.background5 h1{
    padding-top: 50px;
    margin-left: 250px;
}
.expert{
    display: inline-block;
    width: 29%;
    padding-top: 20px;
}

.expert p{
    font-size: 30px;
    line-height: 100px;
    justify-content: left;
    margin-left: 50%;
}
.level{
    display: inline-block;
    width: 69%;
}
